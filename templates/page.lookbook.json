{
  "sections": {
    "hero": {
      "type": "hero",
      "blocks": {
        "heading": {
          "type": "text",
          "settings": {
            "text": "<h1>Lookbook</h1>",
            "type_preset": "h1",
            "max_width": "normal",
            "padding-block-end": 16
          }
        },
        "description": {
          "type": "text",
          "settings": {
            "text": "<p>Explore our latest collection through carefully curated visuals</p>",
            "type_preset": "rte",
            "max_width": "narrow"
          }
        }
      },
      "block_order": ["heading", "description"],
      "settings": {
        "media_type_1": "image",
        "horizontal_alignment_flex_direction_column": "center",
        "vertical_alignment_flex_direction_column": "center",
        "gap": 16,
        "section_height": "medium",
        "color_scheme": "scheme-5",
        "toggle_overlay": true,
        "overlay_color": "#00000040",
        "overlay_style": "gradient",
        "gradient_direction": "to top",
        "padding-block-start": 40,
        "padding-block-end": 40
      }
    },
    "lookbook_main": {
      "type": "lookbook",
      "blocks": {
        "lb_image_1": {
          "type": "image",
          "settings": {
            "span_columns": false,
            "overlay_text": "New Arrivals",
            "link_text": "Shop Now"
          }
        },
        "lb_image_2": {
          "type": "image",
          "settings": {
            "span_columns": true,
            "overlay_text": "Featured Collection",
            "link_text": "Explore"
          }
        },
        "lb_image_3": {
          "type": "image",
          "settings": {
            "span_columns": false
          }
        },
        "lb_image_4": {
          "type": "image",
          "settings": {
            "span_columns": false
          }
        },
        "lb_image_5": {
          "type": "image",
          "settings": {
            "span_columns": false
          }
        },
        "lb_image_6": {
          "type": "image",
          "settings": {
            "span_columns": false,
            "overlay_text": "Handcrafted Details"
          }
        }
      },
      "block_order": ["lb_image_1", "lb_image_2", "lb_image_3", "lb_image_4", "lb_image_5", "lb_image_6"],
      "settings": {
        "heading": "Latest Collection",
        "heading_size": "h2",
        "description": "",
        "layout_style": "masonry",
        "columns_desktop": 3,
        "columns_mobile": "2",
        "gap": 16,
        "enable_overlay": true,
        "section_width": "page-width",
        "color_scheme": "scheme-1",
        "padding-block-start": 60,
        "padding-block-end": 60
      }
    },
    "promo_video": {
      "type": "promo-video",
      "blocks": {
        "heading": {
          "type": "heading",
          "settings": {
            "text": "Experience the Collection",
            "heading_size": "h2"
          }
        },
        "text": {
          "type": "text",
          "settings": {
            "text": "<p>Watch our story unfold</p>"
          }
        }
      },
      "block_order": ["heading", "text"],
      "settings": {
        "video_type": "shopify",
        "autoplay": true,
        "loop": true,
        "muted": true,
        "controls": false,
        "section_height": "70vh",
        "section_height_mobile": "60vh",
        "content_alignment": "align-center",
        "enable_backdrop_blur": true,
        "enable_overlay": true,
        "overlay_color": "rgba(0,0,0,0.4)",
        "color_scheme": "scheme-5",
        "padding-block-start": 0,
        "padding-block-end": 0
      }
    }
  },
  "order": ["hero", "lookbook_main", "promo_video"]
}

